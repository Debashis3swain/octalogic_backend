{
  "name": "octalogic-backend",
  "version": "1.0.0",
  "description": "Vehicle Rental API Backend using NestJS, Passport, Joi, PostgreSQL",
  "main": "dist/main.js",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "seed": "ts-node -r tsconfig-paths/register src/seed/seed.ts",
    "migration:generate": "typeorm migration:generate -d src/migrations",
    "migration:run": "typeorm migration:run"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.7",
    "@nestjs/core": "^10.2.7",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/typeorm": "^10.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "joi": "^17.10.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.2.7",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.5.11",
    "@types/joi": "^17.2.3",
    "@types/node": "^20.17.45",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
